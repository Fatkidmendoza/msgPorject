package edu.msg.flightmanager.backend.dto;

import java.sql.Time;

import edu.msg.flightmanager.backend.model.PeriodicityType;

public class ItineraryDto extends EntityDto {


	private Time duration;
	private PeriodicityType periodicity;

	public Time getDuration() {
		return duration;
	}

	public void setDuration(Time duration) {
		this.duration = duration;
	}
	public PeriodicityType getPeriodicity() {
		return periodicity;
	}
	public void setPeriodicity(PeriodicityType periodicity) {
		this.periodicity = periodicity;
	}

}
