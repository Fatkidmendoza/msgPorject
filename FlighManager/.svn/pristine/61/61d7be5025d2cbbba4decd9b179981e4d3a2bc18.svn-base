<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://xmlns.jcp.org/jsf/facelets"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://xmlns.jcp.org/jsf/html"
	xmlns:p="http://primefaces.org/ui">
<h:head>
	<link href="webcontent/layout.css" rel="stylesheet" type="text/css" />
</h:head>
<h:body>

	<div id="body">
		<header>
				<h1 class="code">Flight Manager</h1>
		</header>

		<section>
				<p:megaMenu orientation="vertical" class="code" >

					<p:submenu label="Flights" icon="ui-icon-document">

						<p:column>
							<p:submenu label="Operations">
								<p:menuitem value="View" url="flights.xhtml" />
								<p:menuitem value="New" url="#" />
							</p:submenu>

						</p:column>

					</p:submenu>

					<p:submenu label="Flight Templates" icon="ui-icon-pencil">

						<p:column>
							<p:submenu label="Operations">
								<p:menuitem value="View" url="flightTemplates.xhtml" />
								<p:menuitem value="New" url="#" />
							</p:submenu>
						</p:column>
					</p:submenu>

					<p:submenu label="Itineraries" icon="ui-icon-contact">
						<p:column>
							<p:submenu label="Operations">
								<p:menuitem value="View" url="itineraries.xhtml" />
								<p:menuitem value="New" url="#" />
							</p:submenu>

						</p:column>
					</p:submenu>

					<p:submenu label="Planes" icon="ui-icon-contact">
						<p:column>
							<p:submenu label="Operations">
								<p:menuitem value="View" url="planes.xhtml" />
								<p:menuitem value="New" url="#" />
							</p:submenu>
						</p:column>
					</p:submenu>

					<p:submenu label="Users" icon="ui-icon-contact">
						<p:column>
							<p:submenu label="Operations">
								<p:menuitem value="View" url="users.xhtml" />
								<p:menuitem value="Tech.1.2" url="#" />
							</p:submenu>

						</p:column>

					</p:submenu>

					<p:submenu label="Companies" icon="ui-icon-contact">
						<p:column>
							<p:submenu label="Operations">
								<p:menuitem value="View" url="companies.xhtml" />
								<p:menuitem value="Tech.1.2" url="#" />
							</p:submenu>

						</p:column>


					</p:submenu>

					<p:menuitem value="Log out" url="login.xhtml" icon="ui-icon-close" />

				</p:megaMenu>
		</section>

		<aside class="rightPanel code">
				<img
					src="http://fc09.deviantart.net/fs71/f/2012/203/6/f/airplane_by_paullus23-d58794k.jpg"
					width="400" height="400" />

		</aside>

		<footer class="code">
			footer
		</footer>
	</div>
</h:body>
</html>